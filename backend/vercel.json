{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/admin/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index?path=$1"
    }
  ],
  "buildCommand": "prisma generate",
  "outputDirectory": "."
}